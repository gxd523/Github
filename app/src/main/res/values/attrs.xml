<resources>
    <declare-styleable name="DetailItemView">
        <attr name="item_title" format="string" />
        <attr name="item_content" format="string" />
        <attr name="item_icon" format="reference" />
        <attr name="item_op_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="AppBarLayoutBehavior">
        <attr name="targetTop" format="dimension" />
        <attr name="targetLeft" format="dimension" />
        <attr name="originalLeft" format="dimension" />
        <attr name="originalTop" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AvatarImageView">
        <attr name="aiv_TextSizeRatio" format="reference|float" />
        <attr name="aiv_TextMaskRatio" format="reference|float" />
        <attr name="aiv_BoarderColor" format="reference|color" />
        <attr name="aiv_TextColor" format="reference|color" />
        <attr name="aiv_BoarderWidth" format="reference|dimension" />
        <attr name="aiv_ShowBoarder" format="reference|boolean" />
        <attr name="aiv_CornerRadius" format="reference|dimension" />
    </declare-styleable>
    <attr name="sideBarBackground" format="reference" />
</resources>